    ["item_name_910"]="Everard",
    ["item_explain_214"]="Daje szansę zdobycia: szkatuły z kosztownościami panny, odświętnej szkatuły panny.",
    ["bigPve_name_42"]="Odeprzeć atak",
    ["item_explain_213"]="Daje szansę zdobycia: mikstury życzliwości, mikstury wytrzymałości, mikstury energii, perfum, sukni balowej.",
    ["bigPve_name_43"]="Dobro królestwa",
    ["bigPve_name_40"]="Rywalizujący królowie",
    ["item_explain_215"]="Daje szansę zdobycia: posagu w srebrze, posagu w złocie, posagu w diamentach.",
    ["bigPve_name_41"]="Podzielone królestwo",
    ["bigPve_name_46"]="Obłęd herezji",
