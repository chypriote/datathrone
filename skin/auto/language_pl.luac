    ["equip.upSucTip"]="Wręczono",
    ["bigPve_name_11"]="Dziwny sojusz",
    ["bigPve_name_12"]="Manewr Le Rouxa",
    ["bigPve_name_17"]="Podziemny atak",
    ["story_txt_635"]="Żołnierze Argendale, czy naprawdę chcecie służyć temu żałosnemu tchórzowi? A może wolicie prawdziwego króla?",
    ["union.pos5"]="Wyrzuć z sojuszu.",
    ["bigPve_name_18"]="Zamek Gildershire",
    ["story_txt_634"]="To moje królestwo! WSZYSTKIE ziemie będą moje! Za kogo ty się uważasz?!",
    ["bigPve_name_15"]="Konwencjonalna strategia",
    ["story_txt_633"]="Twój czas dobiegł końca. Poddaj się natychmiast, a oszczędzimy twoich ludzi. Być może nawet znajdą swoje miejsce w mojej nowej armii.",
    ["bigPve_name_16"]="W głąb lochu",
    ["story_txt_632"]="To nie tak miało być! Chwała BĘDZIE należeć do mnie! Zabijcie ich wszystkich dla mnie! Jestem waszym jedynym prawdziwym królem!",
    ["story_txt_631"]="Mając tylu ludzi, z łatwością okrążymy pozycję wroga. Nie będzie już dla niego ucieczki.",
    ["government.numberJoin"]="Dołącz do rady",
    ["story_txt_630"]="Tak, Magnusie. W samą porę!",
    ["bigPve_name_19"]="Pod moim sztandarem",
