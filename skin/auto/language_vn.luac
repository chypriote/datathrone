    ["item_name_203"]="Lãnh Chúa Chiến Binh",
    ["palace_name_6"]="Vua Khoan Hồng",
    ["help_show_139"]="Ngài có thể nhận EXP vương quốc từ Chiến Dịch hoặc Sắc Lệnh",
    ["palace_name_3"]="Vua Chinh Phạt",
    ["palace_name_4"]="Vua Vinh Quang",
