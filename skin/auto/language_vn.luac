    ["story_say_1"]="Ta",
    ["bigPve_name_77"]="Cuồng Tín",
    ["bigPve_name_78"]="Phản Loạn Tự Phát",
    ["story_txt_657"]="Ngay cả ta cũng không cảm thấy đúng khi cầm vũ khí chống lại họ. Bây giờ ta sẽ dừng kiếm của mình lại. Ta sẽ chiến đấu chỉ để kiểm soát họ, chứ không giết.",
    ["story_txt_656"]="Dù bây giờ họ có sai lầm như thế nào, thì họ cũng đã từng là dân làng bình thường. Có lẽ chúng ta có thể tìm cách cứu họ.",
    ["story_txt_655"]="Chúng ta phải nhớ rằng bất kỳ tên tín đồ cuồng giáo nào đều từng là một dân làng vô tội. Chúng ta cần chấm dứt điều này một cách nhanh chóng trước khi nhiều người phải chịu đau khổ hơn.",
    ["story_txt_654"]="Đi, các tín đồ của ta, và thanh trừng vùng đất của những kẻ báng bổ xâm lược này!",
    ["story_say_8"]="Alain le Roux",
    ["story_txt_653"]="Chúng ta sẽ chấm dứt sự sùng bái của ngươi và dập tắt bất cứ mọi từ ngữ xấu xa mà ngươi đã lan truyền. Sẽ không ai còn nhớ đến ngươi nữa.",
    ["bangdingfb"]="Facebook (%s)",
    ["story_say_9"]="Friar Tuck",
    ["story_txt_652"]="Xúc phạm! Thứ sinh vật độc ác! Đội quân của ta sẽ tấn công ngươi. Vùng đất này sẽ tràn ngập trong máu của ngươi!",
    ["story_say_6"]="Cyrano",
    ["story_txt_651"]="Chúng ta đến đây để ngăn cản các ngươi, chứ không phải gia nhập, đồ điên!",
    ["story_say_7"]="Constable Bertrand",
    ["story_txt_650"]="Ô ho ho! Ngươi muốn được ban phước thiêng liêng một mình. Được, ta đến đây để phục vụ ngươi. Bước về phía trước đi, con trai của ta, và nhận phước lành của ta.",
    ["mk_badges"]=" Cuộn Sách",
    ["story_txt_619"]="Không, những vị vua như hắn chỉ đơn giản là không có ý định cai trị. Ta đã đưa ra quyết định của mình. Ta sẽ lấy vương miện của hắn và cả vương quốc này.",
    ["story_txt_618"]="Ở mọi nơi chúng ta đến, chúng ta đều bị kéo vào trận chiến. Cõi nào lại cho phép mình bị phân mảnh thành như vậy?",
    ["union.pos2"]="Lãnh Chúa",
    ["story_txt_617"]="Tiếng quát tháo và giả vờ dũng cảm của ngươi đều vô nghĩa. Ta có mọi lợi thế.",
    ["union.pos1"]="Minh Chủ",
    ["story_txt_616"]="Ngươi sẽ phải vượt qua chúng ta, và chúng ta không có ý định thua những kẻ như ngươi.",
    ["union.pos4"]="Thành viên",
    ["story_txt_615"]="Thế là vị vua xâm lược lại một lần nữa đặt chân lên đất của ta. Chà, đã đến lúc ta kết thúc cuộc chiến này.",
    ["union.pos3"]="Quý Tộc",
    ["story_txt_614"]="Thầy dòng, hãy dẫn người lục soát tàu và tìm hàng hóa còn sót lại. Những người còn lại sẽ canh gác. Vệ Vương, chúng ta phải mạo hiểm ra bãi biển. Hãy sẵn sàng.",
    ["login.password"]="Mật khẩu giới hạn 4-18 ký tự, gồm chữ và số",
    ["item_explain_201"]="Mở khóa danh hiệu \"Hoàng Đế Vô Địch\" (Hiệu lực 30 ngày)",
    ["item_explain_200"]="Mở khóa danh hiệu 'Vua Tiền Tài' (Hiệu lực 30 ngày)",
