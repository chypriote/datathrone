    ["servant.top.title_2"]="Tăng cấp kỹ năng",
    ["servant.top.title_3"]="Tăng kỹ năng Khế Ước",
    ["story_txt_199"]="Sẽ là ai đây, bệ hạ? Ai sẽ xử lý con chồn này?",
    ["servant.top.title_4"]="Hướng Dẫn",
