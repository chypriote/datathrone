    ["epSkill_name_421"]="Twórca skarbów",
    ["epSkill_name_424"]="Błogosławiony wojownik",
    ["epSkill_name_423"]="Nieustraszona obrończyni",
    ["epSkill_name_426"]="Król banita",
    ["epSkill_name_425"]="Łupy Apolla",
