    ["rebelBoss_name_4"]="Bárbaro",
    ["story_txt_101483"]="Oh vaya, tu llegada es realmente propicia, me siento bendecida.",
    ["pvpText_hitedText_3002"]="Me haces cosquillas.",
    ["rebelBoss_name_5"]="Guerrero",
