    -- local act1 = cc.MoveTo:create(2, cc.p(entity.entity.targetEnemy.position.x,entity.entity.targetEnemy.position.y))
    local act1 = cc.JumpTo:create(1, endPos,(startPos.y+endPos.y)/2+100,1)
