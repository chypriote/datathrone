function UICommon.reallyTime(str,h,m,s)
    local Y = string.sub(str,1,4)
    local M = string.sub(str,5,6)
    local D = string.sub(str,7,8)
    return os.time({year=Y, month=M, day=D, hour=h, min=m, sec=s}) 
end

