function UICommon.splitList(list,num)
    local len  = list and #list or 0
    if len <= 0 then
        return nil
    end
    local slist = {}
    local idx = 1
    while(idx <= len)
    do
        local sl = UICommon.createSplit(list,idx,idx+num-1)
        table.insert(slist,sl)
        idx = idx + num
    end
    return slist
end
function UICommon.createSplit(list,sId,tId)
    local row = {}
    for i=sId,tId do
        local ting = list[i]
        if ting then
            table.insert(row,ting)
        end
    end
    return row
end

