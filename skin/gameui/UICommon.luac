-- noSpine：关闭动画
function UICommon.setWifeDongHuaTEM(img,vo,scale,tuo,noSpine)
    scale = scale or 1
    local oldNode = img:getChildByName("jiaren")
    if oldNode then
        oldNode:removeSelf()
    end

    if noSpine then
        return UICommon.setWifeImageTEM(img,vo,scale,tuo)
    end

    local curSpine = nil
    local skinDir = nil
    if tuo then
